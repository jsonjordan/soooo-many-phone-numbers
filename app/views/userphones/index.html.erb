<h2>Users</h2>
<% @users.each do |u| %>
  <h4><%= u.username %></h4> <%= link_to "Edit", edit_userphone_path(u), class: "btn btn-primary btn-xs" %>
  <ul>
    <% u.phones.each do |p| %>
      <li>
        <%= p.number %>
      </li>
    <% end %>
  </ul>
<% end %>

<h2>New User</h2>
 <%= link_to "New", new_userphone_path, class: "btn btn-primary btn-xs" %>
